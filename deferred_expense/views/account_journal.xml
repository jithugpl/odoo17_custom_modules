<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_account_journal_inherit_deferred_expense" model="ir.ui.view">
        <field name="name">account.journal.inherited</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//page[@name='bank_account']//group//group//field[@name='currency_id']"
                   position="after">
                <field name="deferred_expense"/>
            </xpath>
        </field>
    </record>
</odoo>