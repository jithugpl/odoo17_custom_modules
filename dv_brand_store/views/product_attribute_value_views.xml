<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_attribute_value_form_custom" model="ir.ui.view">
        <field name="name">product.attribute.value.form.custom</field>
        <field name="model">product.attribute</field>
        <field name="inherit_id" ref="product.product_attribute_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='attribute_values']/field[@name='value_ids']/tree" position="inside">
                 <field name="is_active_brand"/>
                 <field name="brand_link"/>
            </xpath>
        </field>
    </record>
</odoo>